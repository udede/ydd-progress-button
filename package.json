{
  "name": "ydd-progress-button",
  "version": "1.1.17",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run build:lib && npm run build:showcase",
    "copy:readme": "copyfiles ./README.md ./projects/progress-button && copyfiles ./README.md ./projects/progress-button-showcase",
    "copy:changelog": "copyfiles ./CHANGELOG.md ./projects/progress-button",
    "build:lib": "npm run copy:readme && npm run copy:changelog && ng build progress-button",
    "build:showcase": "ng build progress-button-showcase --base-href='.' --rebase-root-relative-css-urls=true",
    "publish:showcase": "npx angular-cli-ghpages --dir=./dist/progress-button-showcase",
    "test": "ng test",
    "test-headless": "ng test --code-coverage --watch=false --browsers=ChromeHeadless",
    "version": "standard-version",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.0.9",
    "@angular/cdk": "^11.0.4",
    "@angular/common": "~11.0.9",
    "@angular/compiler": "~11.0.9",
    "@angular/core": "~11.0.9",
    "@angular/forms": "~11.0.9",
    "@angular/platform-browser": "~11.0.9",
    "@angular/platform-browser-dynamic": "~11.0.9",
    "@angular/router": "~11.0.9",
    "@ng-select/ng-select": "^5.0.15",
    "angular-cli-ghpages": "^0.6.2",
    "ngx-markdown": "^11.0.1",
    "ngx-scrollbar": "^7.5.3",
    "prismjs": "^1.23.0",
    "rxjs": "~6.6.3",
    "zone.js": "^0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.7",
    "@angular-devkit/build-ng-packagr": "~0.1002.0",
    "@angular/cli": "~11.0.7",
    "@angular/compiler-cli": "~11.0.9",
    "@angular/language-service": "~11.0.9",
    "@types/jasmine": "~3.6.3",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "~14.14.22",
    "codelyzer": "^6.0.1",
    "copyfiles": "^2.4.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~6.0.0",
    "karma": "~6.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "ng-packagr": "^11.1.2",
    "protractor": "~7.0.0",
    "standard-version": "^9.1.0",
    "ts-node": "~9.1.1",
    "tsickle": "^0.39.1",
    "tslint": "^6.1.3",
    "typescript": "~4.0.0"
  }
}
