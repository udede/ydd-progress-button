{
  "name": "ydd-progress-button",
  "version": "1.2.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run build:lib && npm run build:showcase",
    "copy:readme": "copyfiles ./README.md ./projects/progress-button && copyfiles ./README.md ./projects/progress-button-showcase",
    "copy:changelog": "copyfiles ./CHANGELOG.md ./projects/progress-button",
    "build:lib": "npm run copy:readme && npm run copy:changelog && ng build progress-button",
    "build:showcase": "ng build progress-button-showcase --base-href='.'",
    "publish:showcase": "npx ngh --dir=dist/progress-button-showcase",
    "test": "ng test",
    "test-headless": "ng test --code-coverage --watch=false --browsers=ChromeHeadless",
    "version": "standard-version",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.0.1",
    "@angular/cdk": "^12.0.1",
    "@angular/common": "~12.0.1",
    "@angular/compiler": "~12.0.1",
    "@angular/core": "~12.0.1",
    "@angular/forms": "~12.0.1",
    "@angular/platform-browser": "~12.0.1",
    "@angular/platform-browser-dynamic": "~12.0.1",
    "@angular/router": "~12.0.1",
    "@ng-select/ng-select": "^6.1.0",
    "angular-cli-ghpages": "^0.6.2",
    "ngx-markdown": "^12.0.1",
    "ngx-scrollbar": "^7.5.6",
    "prismjs": "^1.23.0",
    "rxjs": "~7.1.0",
    "zone.js": "^0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.0.4",
    "@angular-devkit/build-ng-packagr": "~0.1002.0",
    "@angular/cli": "~12.0.1",
    "@angular/compiler-cli": "~12.0.1",
    "@angular/language-service": "~12.0.1",
    "@types/jasmine": "~3.7.4",
    "@types/jasminewd2": "~2.0.9",
    "@types/node": "~15.6.0",
    "codelyzer": "^6.0.2",
    "copyfiles": "^2.4.1",
    "jasmine-core": "~3.7.1",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.6.0",
    "ng-packagr": "^12.0.0",
    "protractor": "~7.0.0",
    "standard-version": "^9.3.0",
    "ts-node": "~10.0.0",
    "tsickle": "^0.39.1",
    "tslint": "^6.1.3",
    "typescript": "~4.2.3"
  }
}
