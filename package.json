{
  "name": "ydd-progress-button",
  "version": "1.2.3",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run build:lib && npm run build:showcase",
    "copy:readme": "copyfiles ./README.md ./projects/progress-button && copyfiles ./README.md ./projects/progress-button-showcase",
    "copy:changelog": "copyfiles ./CHANGELOG.md ./projects/progress-button",
    "build:lib": "npm run copy:readme && npm run copy:changelog && ng build progress-button",
    "build:showcase": "ng build progress-button-showcase --base-href='.'",
    "publish:showcase": "npx ngh --dir=dist/progress-button-showcase",
    "test": "ng test",
    "test-headless": "ng test --code-coverage --watch=false --browsers=ChromeHeadless",
    "version": "standard-version",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.3.0",
    "@angular/cdk": "^13.3.0",
    "@angular/common": "~13.3.0",
    "@angular/compiler": "~13.3.0",
    "@angular/core": "~13.3.0",
    "@angular/forms": "~13.3.0",
    "@angular/platform-browser": "~13.3.0",
    "@angular/platform-browser-dynamic": "~13.3.0",
    "@angular/router": "~13.3.0",
    "@ng-select/ng-select": "^8.1.1",
    "angular-cli-ghpages": "^1.0.0",
    "ngx-markdown": "^13.1.0",
    "ngx-scrollbar": "^9.0.2",
    "prismjs": "^1.27.0",
    "rxjs": "~7.5.5",
    "zone.js": "^0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.0",
    "@angular-devkit/build-ng-packagr": "~0.1002.0",
    "@angular/cli": "~13.3.0",
    "@angular/compiler-cli": "~13.3.0",
    "@angular/language-service": "~13.3.0",
    "@types/jasmine": "~4.0.0",
    "@types/jasminewd2": "~2.0.10",
    "@types/node": "~17.0.21",
    "codelyzer": "^6.0.2",
    "copyfiles": "^2.4.1",
    "jasmine-core": "~4.0.1",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.17",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.7.0",
    "ng-packagr": "^13.3.0",
    "protractor": "~7.0.0",
    "standard-version": "^9.3.2",
    "ts-node": "~10.7.0",
    "tsickle": "^0.46.0",
    "tslint": "^6.1.3",
    "typescript": "~4.6.2"
  }
}
