{
  "name": "ydd-progress-button",
  "version": "1.1.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run build:lib && npm run build:showcase",
    "copy:readme": "copyfiles ./README.md ./projects/progress-button ./projects/progress-button-showcase ./dist/progress-button ./dist/progress-button-showcase",
    "build:lib": "npm run copy:readme && ng build progress-button",
    "build:showcase": "ng build progress-button-showcase --base-href='.' --rebase-root-relative-css-urls=true",
    "publish:showcase": "npx angular-cli-ghpages --dir=./dist/progress-button-showcase",
    "test": "ng test",
    "test-headless": "ng test --code-coverage --watch=false --browsers=ChromeHeadless",
    "version": "standard-version",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.5",
    "@angular/cdk": "^8.2.0",
    "@angular/common": "~8.2.5",
    "@angular/compiler": "~8.2.5",
    "@angular/core": "~8.2.5",
    "@angular/forms": "~8.2.5",
    "@angular/platform-browser": "~8.2.5",
    "@angular/platform-browser-dynamic": "~8.2.5",
    "@angular/router": "~8.2.5",
    "@ng-select/ng-select": "^3.0.7",
    "angular-cli-ghpages": "^0.6.0",
    "ngx-markdown": "^8.1.0",
    "ngx-scrollbar": "^6.0.0",
    "prismjs": "^1.17.1",
    "rxjs": "~6.5.3",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.3",
    "@angular-devkit/build-ng-packagr": "~0.803.3",
    "@angular/cli": "~8.3.3",
    "@angular/compiler-cli": "~8.2.5",
    "@angular/language-service": "~8.2.5",
    "@types/jasmine": "~3.4.0",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "~12.7.5",
    "codelyzer": "^5.1.0",
    "copyfiles": "^2.1.1",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "^5.5.0",
    "protractor": "~5.4.2",
    "standard-version": "^7.0.0",
    "ts-node": "~8.3.0",
    "tsickle": "^0.37.0",
    "tslint": "^5.20.0",
    "typescript": "^3.5.3"
  }
}
