{
  "name": "ydd-progress-button",
  "version": "1.1.15",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run build:lib && npm run build:showcase",
    "copy:readme": "copyfiles ./README.md ./projects/progress-button && copyfiles ./README.md ./projects/progress-button-showcase",
    "copy:changelog": "copyfiles ./CHANGELOG.md ./projects/progress-button",
    "build:lib": "npm run copy:readme && npm run copy:changelog && ng build progress-button",
    "build:showcase": "ng build progress-button-showcase --base-href='.' --rebase-root-relative-css-urls=true",
    "publish:showcase": "npx angular-cli-ghpages --dir=./dist/progress-button-showcase",
    "test": "ng test",
    "test-headless": "ng test --code-coverage --watch=false --browsers=ChromeHeadless",
    "version": "standard-version",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.14",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/forms": "~8.2.14",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/router": "~8.2.14",
    "@ng-select/ng-select": "^3.7.2",
    "angular-cli-ghpages": "^0.6.2",
    "ngx-markdown": "^8.2.2",
    "ngx-scrollbar": "^7.1.0",
    "prismjs": "^1.19.0",
    "rxjs": "~6.5.4",
    "zone.js": "^0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.23",
    "@angular-devkit/build-ng-packagr": "~0.803.23",
    "@angular/cli": "~9.1.1",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@types/jasmine": "~3.5.2",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "~13.5.2",
    "codelyzer": "^5.2.1",
    "copyfiles": "^2.2.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.1",
    "karma-jasmine": "~3.1.0",
    "karma-jasmine-html-reporter": "^1.5.1",
    "ng-packagr": "^5.7.1",
    "protractor": "~5.4.3",
    "standard-version": "^7.1.0",
    "ts-node": "~8.6.2",
    "tsickle": "^0.38.0",
    "tslint": "^6.0.0",
    "typescript": "3.5.3"
  }
}
