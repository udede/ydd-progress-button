{
  "name": "ydd-progress-button",
  "version": "1.1.17",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run build:lib && npm run build:showcase",
    "copy:readme": "copyfiles ./README.md ./projects/progress-button && copyfiles ./README.md ./projects/progress-button-showcase",
    "copy:changelog": "copyfiles ./CHANGELOG.md ./projects/progress-button",
    "build:lib": "npm run copy:readme && npm run copy:changelog && ng build progress-button",
    "build:showcase": "ng build progress-button-showcase --base-href='.' --rebase-root-relative-css-urls=true",
    "publish:showcase": "npx angular-cli-ghpages --dir=./dist/progress-button-showcase",
    "test": "ng test",
    "test-headless": "ng test --code-coverage --watch=false --browsers=ChromeHeadless",
    "version": "standard-version",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.9",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "~9.1.9",
    "@angular/compiler": "~9.1.9",
    "@angular/core": "~9.1.9",
    "@angular/forms": "~9.1.9",
    "@angular/platform-browser": "~9.1.9",
    "@angular/platform-browser-dynamic": "~9.1.9",
    "@angular/router": "~9.1.9",
    "@ng-select/ng-select": "^4.0.0",
    "angular-cli-ghpages": "^0.6.2",
    "ngx-markdown": "^9.1.1",
    "ngx-scrollbar": "^7.2.3",
    "prismjs": "^1.20.0",
    "rxjs": "~6.5.5",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.7",
    "@angular-devkit/build-ng-packagr": "~0.901.7",
    "@angular/cli": "~9.1.7",
    "@angular/compiler-cli": "~9.1.9",
    "@angular/language-service": "~11.0.1",
    "@types/jasmine": "~3.5.10",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "~14.0.9",
    "codelyzer": "^5.2.2",
    "copyfiles": "^2.3.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.2",
    "karma": "~5.0.9",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~3.3.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "ng-packagr": "^9.1.5",
    "protractor": "~7.0.0",
    "standard-version": "^8.0.0",
    "ts-node": "~8.10.2",
    "tsickle": "^0.38.1",
    "tslint": "^6.1.2",
    "typescript": "3.6.4"
  }
}
